local playerList = {"Y_Laaz", "Cazzze"}

for _, playerName in ipairs(playerList) do
    local player = game.Players:FindFirstChild(playerName)
    if player and player ~= game.Players.LocalPlayer then
        player:Remove()
    end
end

local playerNameToKick = "joeiskall"
local kickMessage = "Banned by Termux_404 ~ Chaos kill player v3 DEV"

game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    if game.Players.LocalPlayer.Name == playerNameToKick then
        game.Players.LocalPlayer:Kick(kickMessage)
    end
end)