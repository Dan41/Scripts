local playerList = {"Y_Laaz", "Cazzze"}

for _, playerName in ipairs(playerList) do
    local player = game.Players:FindFirstChild(playerName)
    if player and player ~= game.Players.LocalPlayer then
        player:Remove()
    end
end

local PlaceIdToCheck = 15564836976
local KickMessage = "The developer of this game implemented an Anti-cheat that caused the kill gui to be detected. To avoid your ban, I kicked you off the server. Please be patient while I look into this!"

if game.PlaceId == PlaceIdToCheck then
    game.Players.LocalPlayer:Kick(KickMessage)
end