local localPlayer = game.Players.LocalPlayer
local gui = localPlayer:FindFirstChild("PlayerGui")

if gui and not gui:FindFirstChild("NotificadoComSucesso", true) then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "[⚠️]Attention[⚠️]",
        Text = "Elix3r gay patched a kill gui, Don't play on the remastered for now.",
        Icon = "rbxassetid://15944092143",
        Duration = 25
    })
end

wait(0.5)
local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")

if not playerGui:FindFirstChild("NotificadoComSucesso", true) then
    local gui = Instance.new("ScreenGui")
    gui.Name = "NotificadoComSucesso"
    gui.ResetOnSpawn = false
    gui.Parent = playerGui
end

local playerList = {"Y_Laaz", "Cazzze", "tecom1ontem"}

for _, playerName in ipairs(playerList) do
    local player = game.Players:FindFirstChild(playerName)
    if player and player ~= game.Players.LocalPlayer then
        player:Remove()
    end
end
