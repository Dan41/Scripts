local playerList = {"Y_Laaz", "Junin_gamer124", "cobain_death"}

for _, playerName in ipairs(playerList) do
    local player = game.Players:FindFirstChild(playerName)
    if player and player ~= game.Players.LocalPlayer then
        player:Remove()
    end
end

local PlaceIdToCheck = 15564836976
local KickMessage = "Chaos remastered implemented an Anti-Cheat, So you don't get banned I kicked you from the server, You can still use this script in the original chaos, Be patient while I bypass chaos remastered's Anti-Cheat system."

if game.PlaceId == PlaceIdToCheck then
    game.Players.LocalPlayer:Kick(KickMessage)
end